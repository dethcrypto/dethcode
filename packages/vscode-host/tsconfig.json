{
  "compilerOptions": {
    "module": "amd",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "allowUnreachableCode": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "vs/*": ["./vscode/src/vs/*"]
    }
  },
  "include": ["src", "./vscode/src/vs/vscode.d.ts"]
}
