{
  "name": "ethereum-viewer",
  "license": "MIT",
  "version": "0.1.0",
  "browser": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run build:prod",
    "build": "webpack",
    "build:prod": "webpack --mode production --devtool hidden-source-map",
    "watch": "webpack --watch",
    "pretest": "yarn run build",
    "lint": "eslint src --ext ts",
    "serve": "serve --cors -l 5000 --ssl-cert ../../certs/localhost.pem --ssl-key ../../certs/localhost-key.pem",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "dependencies": {
    "fast-json-stable-stringify": "^2.1.0",
    "match-sorter": "^6.3.1",
    "p-finally": "^2.0.1",
    "path-browserify": "^1.0.1",
    "ts-essentials": "^9.0.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.63.0",
    "typescript": "^4.5.3"
  },
  "engines": {
    "vscode": "^1.63.0"
  },
  "displayName": "Ethereum Code Viewer",
  "description": "View source of deployed Ethereum contracts in VSCode",
  "publisher": "hasparus",
  "categories": [
    "Other"
  ],
  "enableProposedApi": true,
  "enabledApiProposals": [
    "fileSearchProvider",
    "textSearchProvider"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": {
      "command": "ethereum-viewer.open",
      "category": "Ethereum Code Viewer",
      "title": "Open smart contract source code"
    }
  }
}
